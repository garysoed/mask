{
  "extends": "devbase/ts",
  "compilerOptions": {
    "baseUrl": ".",
    "lib": [
      "dom",
      "es2015",
      "es2020.string"
    ],
    "outDir": "out",
    "paths": {
      "rxjs": ["node_modules/rxjs"],
      "rxjs/*": ["node_modules/rxjs/*"],
    },
  },
  "include": [
    "demo/**/*.ts",
    "src/**/*.ts",
    "src/cli/*.ts",
    "node_modules/devbase/ts/typings/*.d.ts",
  ]
}
